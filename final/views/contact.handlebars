<script>
  var vol = 0;
    const wage = document.getElementById('body1');
    wage.addEventListener('keydown', (e) => {
        if (e.key === 'Enter') {
            vol = 20;
            speak('Hello there, Your feedback is highly appreciated for the Conectar Platform...',vol);
            speak('We ensure to resolve your query.',vol);
        }
    });
  function speak(text1,v) {
      var msg = new SpeechSynthesisUtterance();
      var voices = window.speechSynthesis.getVoices()
      console.log(voices);
      msg.volume = v;
      msg.rate = 1.5;
      msg.pitch = 10;
      msg.text = text1;
      window.speechSynthesis.speak(msg);
    };
</script>
